            <xml-property name="queryText"><![CDATA[select
	dc.id, 
	dc.decision_code, 
	dc.decision_date, 
	dc.status as 'decision_status', 
	dc.motif_rejet, 
	avn.id as 'avancement_id', 
	emp.lastname as 'employe_last_name', 
	emp.firstname as 'employe_first_name',
	emp.matricule as 'employe_matricule',
	fonc.name as 'employe_fonction' 
	from rh_droit_avancement as avn 
join config_decision as dc on dc.id=avn.decision
join rh_employe as emp on emp.id=avn.employee
left join rh_affectation as aff on aff.employee=emp.id and aff.status=3 and aff.type_affectation=1
left join config_fonction as fonc on fonc.id=aff.fonction
where avn.exercice=? and avn.status=?]]></xml-property>
