<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">

    <module name="rh" package="com.axelor.rh.db"/>

    <entity name="Diplome"  cachable="true">
        <string name="name" title="Diplôme"/>
        <string name="niveauScolaire" title="Niveau scolaire"/>
        <date name="dateDiplome" title="Date diplôme"  />
        <string name="titre" title="Titre"  />
        <string name="specialite" title="Spécialité"  />
        <string name="etablissement" title="Etablissement"  />
        <many-to-one name="employee" ref="com.axelor.rh.db.Employe" title="Agent"></many-to-one>
    </entity>

</domain-models>