<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.1.xsd">

    <selection name="employee.title.type.select">
        <option value="MONSIEUR">MONSIEUR</option>
        <option value="MADAME">MADAME</option>
        <option value="MADEMOISELLE">MADEMOISELLE</option>
    </selection>
    <selection name="employee.sex.type.select">
        <option value="M">Homme</option>
        <option value="F">Femme</option>
    </selection>
    <selection name="employee.sexar.type.select">
        <option value="M">ذكر</option>
        <option value="F">أنثى</option>
    </selection>
    <selection name="employee.status.type.select">
        <option value="C">CDC</option>
        <option value="D">DETACHE</option>
        <option value="H">HONORAIRE</option>
        <option value="S">STAGIERE</option>
        <option value="T">TITULAIRE</option>
    </selection>
    <selection name="employee.civilStatus.type.select">
        <option value="M" order="1">Marié</option>
        <option value="C" order="0">Célébataire</option>
        <option value="V" order="2">Veuf(ve)</option>
        <option value="A" order="3">Autre</option>
    </selection>
    <selection name="employee.depart.type.select">
        <option value="DEC">Décés</option>
        <option value="DEM">Démission</option>
        <option value="RET">Retraite</option>
    </selection>

    <selection name="affectation.type.select">
        <option value="1">Principale</option>
        <option value="2">Secondaire</option>
        <option value="3">Abrogé</option>
    </selection>
    <selection name="select-avancement-status">
        <option value="0">Brouillon</option>
        <option value="1">Vérifié</option>
        <option value="2">En attente de validation</option>
        <option value="3">Validé</option>
    </selection>
    <selection name="situation-natures">
        <option value="RECRUTEMENT">RECRUTEMENT</option>
        <option value="AVANCEMENT">AVANCEMENT</option>
        <option value="RECLASSEMENT">RECLASSEMENT</option>
    </selection>
    <selection name="situation-reclassement-natures"  >
        <option value="CHO">Choix</option>
        <option value="CHQ">Choix avec quota</option>
        <option value="CHS">Choix sans quota</option>
        <option value="CON">CON</option>
        <option value="DIP">DIP</option>
        <option value="EAP">EAP</option>
        <option value="EXC">EXC</option>
    </selection>
    <selection name="ouissam-type">
        <option value="EXP">Ouissam de mérite national Exceptionnel</option>
        <option value="1GR">Ouissam de mérite national de 1er Grade</option>
        <option value="2GR">Ouissam de mérite national de 2ème Grade</option>
    </selection>
    <selection name="conge.start.select">
        <option value="1">Matin</option>
        <option value="2">Après-midi</option>
    </selection>

    <selection name="report.types.select">
        <option value="AgentsByEntities" order="1">Situation des agents par entité</option>
        <option value="AgentsByLocality" order="2">Situation des agents par localité</option>
        <option value="EffectifByGrade" order="3">Effectif des personnels par grade</option>
        <option value="EffectifByCadre" order="4">Effectif des personnellle par cadre</option>
        <option value="AgentsBenificiereMed" order="5">Agents benéificières de la mise en disponibilité</option>
        <option value="SituationByCadreSexe" order="6">Situation du personnel par cadre et sexe</option>
        <option value="SituationProfAgents" order="7">Situation profetionnelle des agents</option>
        <option value="SituationByAgeRange" order="8">Situation des agents par tranche d'age</option>
        <option value="SituationGlobalsPersonnel" order="9">Situation global du personnel</option>
        <option value="PersonnelByCsfs" order="10">Situation du personnel par cadre, sexe, filiere, service, statut</option>
        <option value="SituationPersonnelDetache" order="11">Situation des personnels détachés</option>
        <option value="SituationPersonnelContractuel" order="12">Situation des personnels contractuel</option>
        <option value="OrmvahPersonnels" order="13">Liste des reponsables de l'ormva du HAOUZ</option>
    </selection>

</object-views>