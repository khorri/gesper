<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">

    <grid name="ouissam-grid" title="Ouissams" model="com.axelor.rh.db.Ouissam" edit-icon="true">
        <field name="name" selection="ouissam-type" title="Type"/>
        <field name="dateOuissam" title="Date"/>
    </grid>

    <form name="ouissam-form" title="Ouissam" model="com.axelor.rh.db.Ouissam">
        <panel>
            <field name="name" selection="ouissam-type"/>
            <field name="dateOuissam" title="Date"/>
        </panel>
    </form>
</object-views>