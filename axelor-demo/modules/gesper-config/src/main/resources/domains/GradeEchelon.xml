<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">

    <module name="config" package="com.axelor.config.db"/>

    <entity name="GradeEchelon" cachable="true">
        <integer name="echelon" namecolumn="true"/>
        <integer name="rythmeLent"/>
        <integer name="rythmeMoyen"/>
        <integer name="rythmeRapide"/>
        <integer name="indice"/>
        <many-to-one name="grade" ref="Grade" title="Grade"/>

    </entity>

</domain-models>