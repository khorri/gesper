<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">

    <grid name="status-grid" title="Status"
          model="com.axelor.config.db.Status">
        <field name="statusCode" />
        <field name="statusLib"/>
        <field name="statusType"/>
    </grid>
    <selection name="status.statusLib">
        <option value="CDC">CDC</option>
        <option value="DETACHE">DETACHE</option>
        <option value="HONORAIRE">HONORAIRE</option>
        <option value="STAGIERE">STAGIERE</option>
        <option value="TITULAIRE">TITULAIRE</option>
    </selection>
    <selection name="status.statusType">
        <option value="value1">type1</option>
        <option value="value2">type2</option>

    </selection>


    <form name="status-form" title="Status"
          model="com.axelor.config.db.Status">
        <panel title="Overview">
            <field name="statusCode"/>
            <field name="statusLib" selection="status.statusLib" />
            <field name="statusType" selection="status.statusType"/>

        </panel>

    </form>

</object-views>