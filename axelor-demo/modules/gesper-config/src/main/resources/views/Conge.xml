              xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">

    <grid name="conge-grid" title="Congé"
          model="com.axelor.config.db.Conge">
        <field name="codeConge" />
