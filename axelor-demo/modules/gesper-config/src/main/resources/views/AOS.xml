<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">

    <grid name="aos-grid" title="A."
          model="com.axelor.config.db.Aos">
        <field name="affiliationNumero" />
        <field name="dateAffiliation"/>
        <field name="datecession"/>
        <field name="status"/>
    </grid>

    <form name="aos-form" title="A.O.S"
          model="com.axelor.config.db.Aos">
        <panel title="Overview">
            <field name="affiliationNumero" title="Numero d'affiliation" />
            <field name="dateAffiliation" title="Date d'affiliation"/>
            <field name="datecession" title="Date Cession"/>
            <field name="status" title="Status"/>

        </panel>

    </form>

</object-views>